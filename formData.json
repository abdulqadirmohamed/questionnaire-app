{
    "display": "form",
    "components": [
      {
        "label": "First Name",
        "tableView": true,
        "key": "name",
        "type": "textfield",
        "input": true
      },
      {
        "label": "Last Name",
        "tableView": true,
        "key": "lastName",
        "type": "textfield",
        "input": true
      },
      {
        "label": "Plan",
        "widget": "choicesjs",
        "tableView": true,
        "data": {
          "values": [
            {
              "label": "Free",
              "value": "free"
            },
            {
              "label": "Paid",
              "value": "paid"
            }
          ]
        },
        "selectThreshold": 0.3,
        "key": "select",
        "type": "select",
        "indexeddb": {
          "filter": {}
        },
        "input": true,
        "defaultValue": "free"
      },
      {
        "label": "Phone Number",
        "tableView": true,
        "key": "phoneNumber",
        "type": "phoneNumber",
        "input": true
      },
      {
        "label": "Email",
        "tableView": true,
        "key": "email",
        "type": "email",
        "input": true
      },
      {
        "type": "button",
        "label": "Submit",
        "key": "submit",
        "disableOnInvalid": true,
        "input": true,
        "tableView": false
      }
    ]
}  